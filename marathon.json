{
    "container": {
        "docker": {
            "image": "sash/microservice-a:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "hostPort": 0,
                    "containerPort": 8088,
                    "protocol": "tcp",
                    "servicePort": 3333
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "healthChecks": [
        {
            "gracePeriodSeconds": 600,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 2,
            "portIndex": 0,
            "protocol": "TCP",
            "timeoutSeconds": 10
        }
    ],
    "id": "microservice-a",
    "instances": 1,
    "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_DEPLOYMENT_GROUP": "microservices"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}